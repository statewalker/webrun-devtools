{
  "name": "@statewalker/webrun-devtools",
  "description": "DevTools Extension for the StateWalker WebRun framework",
  "version": "0.0.5",
  "type": "module",
  "homepage": "https://github.com/statewalker/webrun-devtools",
  "author": {
    "name": "Mikhail Kotelnikov",
    "email": "mikhail.kotelnikov@gmail.com"
  },
  "license": "MIT",
  "module": "dist/webrun-devtools.js",
  "main": "dist/webrun-devtools.js",
  "scripts": {
    "prepublish": "yarn zip",
    "dev": "tsc && nodemon",
    "zip": "yarn build && cd ./dist/extension && zip -r ../webrun-devtools-extension.zip *",
    "build": "tsc && vite build --config ./vite.config.views.js && vite build --config ./vite.config.content.js && vite build --config ./vite.config.background.js && vite build --config ./vite.config.api.js"
  },
  "files": [
    "dist/*.js",
    "dist/*.zip",
    "src/**",
    "examples/**",
    "README.md"
  ],
  "devDependencies": {
    "@statewalker/getset": "^0.2.4",
    "@statewalker/utils": "^0.4.2",
    "idb-keyval": "^6.2.1",
    "nodemon": "^3.0.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:statewalker/webrun-devtools.git"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  }
}
